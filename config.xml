<?xml version='1.0' encoding='utf-8'?>
<widget id="" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Despesas App</name>
    <description>
        Despesas.
    </description>
    <platform name="android">
        <icon density="ldpi" src="src/assets/cordova/res/icons/android/icon-36-ldpi.png" />
        <icon density="mdpi" src="src/assets/cordova/res/icons/android/icon-48-mdpi.png" />
        <icon density="hdpi" src="src/assets/cordova/res/icons/android/icon-72-hdpi.png" />
        <icon density="xhdpi" src="src/assets/cordova/res/icons/android/icon-96-xhdpi.png" />
        <icon density="xxhdpi" src="src/assets/cordova/res/icons/android/icon-144-xxhdpi.png" />
        <icon density="xxxhdpi" src="src/assets/cordova/res/icons/android/icon-192-xxxhdpi.png" />
        <splash density="port-ldpi" src="src/assets/cordova/res/screens/android/screen-ldpi-portrait.png" />
        <splash density="port-mdpi" src="src/assets/cordova/res/screens/android/screen-mdpi-portrait.png" />
        <splash density="port-hdpi" src="src/assets/cordova/res/screens/android/screen-hdpi-portrait.png" />
        <splash density="port-xhdpi" src="src/assets/cordova/res/screens/android/screen-xhdpi-portrait.png" />
        <splash density="land-ldpi" src="src/assets/cordova/res/screens/android/screen-ldpi-landscape.png" />
        <splash density="land-mdpi" src="src/assets/cordova/res/screens/android/screen-mdpi-landscape.png" />
        <splash density="land-hdpi" src="src/assets/cordova/res/screens/android/screen-hdpi-landscape.png" />
        <splash density="land-xhdpi" src="src/assets/cordova/res/screens/android/screen-xhdpi-landscape.png" />
    </platform>
    <preference name="SplashScreenDelay" value="2000" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-statusbar" spec="~2.1.2" />
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.1" />
    <engine name="ios" spec="~4.1.0" />
    <engine name="android" spec="~5.1.1" />
    <engine name="windows" spec="~4.3.1" />
    <plugin name="cordova-plugin-file" spec="~4.2.0" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.5.1" />
</widget>
